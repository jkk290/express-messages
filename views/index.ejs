<html>
    <head>
        <link rel="stylesheet" href="/styles.css">
        <title><%= title %></title>
    </head>
    <body>
        <ul>
            <% messages.forEach((message) => { %>
                <li>
                    <a href="/<%= message.id %>" class="users"><%= message.user %></a>
                    <p class="messages"><%= message.text %></p>
                </li>
            <% }); %>
        </ul>
        <a href="/new" class="newMessageBtn">New message</a>
    </body>
</html>