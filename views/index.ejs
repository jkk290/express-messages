<html>
    <head>
        <title><% title %></title>
    </head>
    <body>
        <ul>
            <% messages.forEach((message) => { %>
                <li>
                    <a href="/<%= message.id %>"><%= message.user %></a>
                    <p><%= message.text %></p>
                </li>
            <% }); %>
        </ul>
        <a href="/new">New message</a>
    </body>
</html>